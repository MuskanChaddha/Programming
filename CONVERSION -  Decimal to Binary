//Binary To Decimal

#include<stdio.h>

int binary(int n)
{
    int rem,sum=0,i=1;
    
    while(n!=0)
    {
        rem=n%2;
        printf("%d\n",rem);
        sum=sum+(rem*i);
        n=n/2;
        printf("\n%d\n",n);
        i=i*10;
    }
    return sum;
    
}

int main()
{
    int n;
    
    scanf("%d",&n);
    
    printf("%d",binary(n));
}
