//Increment Value of an Array

#include<stdio.h>
int increment_elements(int a[],int n) 
{
    int i;
    
    for(i=0;i<n;i++)
     a[i]++;
    return a[i];
}

int main()
{
    int n,a[100],i;
    
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
     scanf("%d",&a[i]);
     
    
    increment_elements(a,n);
    
    for(i=0;i<n;i++) 
     printf("%d",a[i]); 
     
}

